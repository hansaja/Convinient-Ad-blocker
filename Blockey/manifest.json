{
    "manifest_version" : 2,
    "name": "Blockey",
    "description": "Blockings ads and help you to be secure in the Internet ",
    "version" : "2.0.0",
    "options_page": "more.html",
    "icons": {
        "128" : "icons_128.png",
        "16" : "icon.png"
    },
    "content_scripts": [
        {
          "matches": ["https://*.youtube.com/*"],
          "js": ["youtube.js"]
        }
      ],
    "background": {
        "scripts": ["blockedSites.js","background.js"]      
          
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "contextMenus",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "tabs",
        "storage"
    ]
}