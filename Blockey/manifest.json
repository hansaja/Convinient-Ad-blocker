{
    "manifest_version" : 2,
    "name": "Blockey",
    "description": "Blockings ads and help to be secure in wwb",
    "version" : "1.0.0",
    "icons": {
        "128" : "icons_128.png",
        "16" : "icon.png"
    },
    "background": {
        "scripts": ["blockedSites.js","background.js"]
       
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "contextMenus",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "tabs"
    ]
}